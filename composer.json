{
    "name": "bigcommerce/mock-injector",
    "type": "library",
    "description": "Auto-mocking injector for testing DI components",
    "keywords": [
        "di",
        "pimple",
        "injector",
        "test"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Benson",
            "email": "adam.benson@bigcommerce.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "ssh://git@github.com/bigcommerce-labs/injector.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "bigcommerce/injector": "@stable",
        "phpunit/phpunit": "^6.0 || ^5.6"
    },
    "autoload": {
        "psr-4": {
            "Bigcommerce\\MockInjector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
